class Solution:
    def addBinary(self, a: str, b: str) -> str:
        c=0
        d=0
        for i in range(0,len(a)):
            c+=int(a[i])(2*(len(a)-i-1))
        for i in range(0,len(b)):
            d+=int(b[i])(2*(len(b)-i-1))
        
        s=str(bin(c+d))
        
        return s[2:]
