class Solution:
    def reverse(self, x: int) -> int:
        y=x
        t=len(str(x))-1
        if x<0:
            t=len(str(x))-2
            y=-1*y
        
        out=0

        while(y!=0):
            out=out+(y%10)*(10**t)
            t=t-1
            y=y//10

            
        if out not in range(-2**31, 2**31 - 1):
            return 0
        if x<0:
            return(-1*out)    

        return out
